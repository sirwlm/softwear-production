<dl>
  <dt>Machine</dt>
  <dd><%= fba_bagging_train.machine.try(:name) || '<None>' %></dd>

  <dt>Estimated Time</dt>
  <dd><%= fba_bagging_train.estimated_time || '<not set>' %> hours</dd>

  <dt>Due Date</dt>
  <dd><%= fba_bagging_train.due_date.strftime('%m/%d/%Y') %></dd>

  <dt>Quantity</dt>
  <dd><%= fba_bagging_train.quantity %></dd>

  <dt>Inventory Location</dt>
  <dd><%= fba_bagging_train.inventory_location %></dd>

  <dt>Printed</dt>
  <dd><%= fba_bagging_train.printed? %></dd>

  <dt>Imprint(s) Scheduled At</dt>
  <% fba_bagging_train.order.production_trains.each do |imprint| %>
      <dd class="screen-train-imprints"><%= imprint.name %> - <%= imprint.scheduled_at.strftime('%Y-%m-%d %I:%M%P') rescue "No Scheduled Time"%></dd>
  <% end %>
</dl>
