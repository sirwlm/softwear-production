$('#imprint-group-<%= @imprint_group.id %>').remove();
<% @imprints.each do |imprint| %>
  $('#imprint-<%= imprint.id %>').html(
      "<%= js_train_entry(imprint) do %>
          <%- imprint.reload -%>
          <%= render 'shared/imprint_details', object: imprint %>
        <% end -%>"
  );
<% end %>

initializeDraggableImprints();
