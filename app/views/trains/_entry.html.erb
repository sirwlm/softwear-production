<div class="col-xs-4">
    <div class='draggable-imprint panel panel-primary' data-imprint-id="<%= object.id %>">
    <div class="panel-heading">
      <% if object.respond_to?(:name) %>
        <%= object.name %>
      <% else %>
        <%= object.model_name.element.humanize %>
      <% end %>
    </div>

    <div class="panel-body"> 
    <dl>
      <dt>State</dt>
      <dd class='<%= object.class.name.underscore %>_<%= object.id %>_current_state'><%= object.send("human_#{object.train_machine.attribute}_name") %></dd>
    </dl>

    <%= capture(&block) if local_assigns[:block] %>
      <% if object.respond_to?(:part_of_group?) && object.part_of_group? %>
      <%= link_to 'Show Full Details', imprint_group_path(object.imprint_group), remote: true, class: 'btn btn-primary' %>
    <% else %>
      <%= link_to 'Show Full Details', show_train_path(object), remote: true, class: 'btn btn-primary' %>
    <% end %>
    </div>
  </div>
</div>
