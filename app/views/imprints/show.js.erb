$('#contentModal .modal-title').html("<%= @imprint.full_name %>");
$('#contentModal .modal-body').html("<%=j render('imprints/view', imprint: @imprint, edit_link: true) %>");
$('#contentModal .modal-dialog').css('width', '90%');
$('#contentModal').modal('show');
$('#contentModalSaveChanges').hide();
$('.select2').select2({width: '65%'});
<% if current_user.admin? %>
  $('.editable').editable({
  });
<% end %>

<% if local_assigns[:refresh_imprint] %>
  $(window.calendarMatcher).fullCalendar(
    'removeEvents', <%= @imprint.event_id %>
  );
  $(window.calendarMatcher).fullCalendar(
    'renderEvent', JSON.parse("<%=j render(partial: 'machines/calendar_entry', locals: { event: @imprint }, formats: [:json]).html_safe %>")
  );
<% end %>
