if (window.calendarMatcher != undefined) {
  <% if session[:show_machines].try(:keys).try(:include?, @imprint.machine_id.to_s) %>
  changeEventColor(window.calendarMatcher, <%= @imprint.id %>, "<%= @imprint.calendar_color %>");
  <% else %>
  $(window.calendarMatcher).fullCalendar('removeEvents', <%= @imprint.id %>);
  <% end %>
}
