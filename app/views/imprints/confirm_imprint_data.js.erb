<% if request.post? %>
    $('#contentModal').modal('hide');
    $("#imprint_<%= @imprint.id %>_print_data").html("<%=j render('shared/imprint_data/confirmed_data', event: @imprint) %>");
<% else %>
    try {
        $('#contentModal .modal-title').html("<%=j @title %>");
        $('#contentModal .modal-body').html("<%=j render('shared/imprint_data/form', imprint: @imprint) %>");
        $('#contentModal .modal-dialog').css('width', '90%');
        $('#contentModal').modal('show');
        $('#contentModalSaveChanges').hide();
    }
    catch(e) {
        console.error(e);
    }
<% end %>