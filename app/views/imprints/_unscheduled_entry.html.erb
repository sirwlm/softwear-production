<%= link_to imprint_path(imprint.id), remote: true, class: 'event-drop unscheduled-imprint-link', data: { id: imprint.id } do %>
  <div class="unscheduled-imprint ui-draggable ui-draggable-handle" style="background-color: <%= imprint.calendar_color %>;">
    <% if imprint.estimated? %>
      <%= imprint.name %> <span class='estimated-time'><%= sprintf('%0.01f', imprint.estimated_time) %></span>hrs
    <% end %>
  </div>
<% end %>
